<%= stylesheet 'states','home'%>
<%= javascript 'jquery/jquery.corner','home'%>
<div id="home" class="clearfix">
	
	<div id="side">
		<div id="sub_link">
			<%= render :partial => 'sub_link' %>
		</div>
	</div>
	
	<div id="main" class="clearfix">
		<div id="home-body">
			<div id="state_box">
				<%= render :partial => 'state_panel' %>
			</div>
			<div id="feeds">
				<% for feed in @feeds %>
					<%= feed.user_id%>
				<% end -%>
			</div>
		</div>
		<div id="home-side">
			<div id="friends">
				<% for user in @user.followed_users%>
				<div class="friends">
					<div class="avatar">
						<%= avatar(user,"50_50")%>
					</div>
					<div class="username">
						<%= user.username%>
					</div>
				</div>
				
				
				<% end %>
			</div>
		</div>
	</div>
	
</div>